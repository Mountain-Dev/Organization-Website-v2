@model System.Collections.ArrayList
@{
    ViewBag.Title = "Home";
    List<organization> orgList = (List<organization>)Model[0];
    List<person> personList = (List<person>)Model[1];
    List<connection> connectionList = (List<connection>)Model[2];
}
@section stylesheet
    {
    <link rel="stylesheet" href="~/CSS/index.css" />
}

<div class="jumbotron">
    <h2>Organizations:</h2>
    <hr />
    <div class="list-group">
        @foreach (var item in ((List<organization>)Model[0]))
            {
                organization org = (organization)item;
            <a href="#" class="list-group-item">
                @org.organization_name
            </a>
        }
    </div>
</div>

